import UIKit

class Car {
    
    var topSpeed = 200
    
    func drive ()   {
        print("driving at \(topSpeed)")
    }
}

class Futurecar : Car {
    
    override func drive() {
        super.drive()
        print("and boosting at 50)")
    }
    
    func fly()  {
        print("flying")
    }
}

let myRide = Car()
myRide.topSpeed
myRide.drive()

let myNewRide = Futurecar()
myNewRide.drive()

